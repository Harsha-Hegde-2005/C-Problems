//Solution by Harsha Hegde : 

#include <stdio.h>

int main() {
	int T,X,Y,Z;
	scanf("%d",&T);
	for(int i=0;i<T;i++)
	{
	    scanf("%d %d %d",&X,&Y,&Z);
	    if(2*Z>X*Y) //To check whether pass percentage is greater than 0.5
	    printf("Yes \n");
	    else
	    printf("No \n");
	}
return 0;
}

