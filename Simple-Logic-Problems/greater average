// this problem is from CodeChef
//Problem : https://www.codechef.com/practice/course/logical-problems/DIFF800/problems/AVGPROBLEM
//Solution by Harsha Hegde :

 #include <stdio.h>

int main() {
	int n,a,b,c;
	float avg=0;
	scanf("%d", &n);
	for(int i=0;i<n;i++)
	{
	    scanf("%d%d%d",&a,&b,&c);
	    avg=(a+b)/2.0;
	    if(avg > c)
	    printf("YES");
	    else
	    printf("NO");
	    printf("\n");
	}
return 0;
}

